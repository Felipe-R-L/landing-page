{
  "public": "dist/browser",
  "i18n": {
    "locales": ["pt", "en-US"],
    "defaultLocale": "pt"
  },
  "rewrites": [
    {
      "source": "/:locale(pt|en-US)",
      "destination": "/:locale/index.html"
    },
    {
      "source": "/:locale(pt|en-US)/:path*",
      "destination": "/:locale/:path*"
    },
    {
      "source": "/",
      "destination": "/pt/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/pt/:path*"
    }
  ]
}
