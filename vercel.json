{
  "rewrites": [
    {
      "source": "/en-US/",
      "destination": "/en-US/index.html"
    },
    {
      "source": "/en-US/(.*)",
      "destination": "/en-US/index.html"
    },
    {
      "source": "/",
      "destination": "/en-US/index.html"
    },
    {
      "source": "/en-US/:path((?!.*\\.).*)",
      "destination": "/en-US/index.html"
    }
  ]
}
