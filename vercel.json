{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "public": "dist/browser",
  "rewrites": [
    {
      "source": "/pt/:path*",
      "destination": "/pt/:path*"
    },
    {
      "source": "/en-US/:path*",
      "destination": "/en-US/:path*"
    },
    {
      "source": "/",
      "destination": "/pt/index.html"
    },

    {
      "source": "/:path*",
      "destination": "/pt-BR/:path*"
    }
  ]
}
