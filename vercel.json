{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "dist/landing-page/pt/:path*",
      "destination": "/pt/:path*"
    },
    {
      "source": "dist/landing-page/en-US/:path*",
      "destination": "dist/landing-page/en-US/:path*"
    },
    {
      "source": "dist/landing-page/",
      "destination": "/pt/index.html"
    },

    {
      "source": "/:path*",
      "destination": "/pt-BR/:path*"
    }
  ]
}
